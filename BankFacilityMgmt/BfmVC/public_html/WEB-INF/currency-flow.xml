<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="currency-flow">
    <default-activity>CurrencySearchPg</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="CurrencySearchPg">
      <page>/bfm/CurrencySearchPg.jsff</page>
    </view>
    <view id="CurrencyPg">
      <page>/bfm/CurrencyPg.jsff</page>
    </view>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>CurrencySearchPg</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>edit</from-outcome>
        <to-activity-id>CurrencyPg</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__3">
        <from-outcome>new</from-outcome>
        <to-activity-id>CreateInsert</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>CreateInsert</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>CreateInsert</from-outcome>
        <to-activity-id>CurrencyPg</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>search</from-outcome>
        <to-activity-id>CurrencySearchPg</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
