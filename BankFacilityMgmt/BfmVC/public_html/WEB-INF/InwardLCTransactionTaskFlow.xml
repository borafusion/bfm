<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="InwardLCTransactionTaskFlow">
    <default-activity>SearchInwardLCTransaction</default-activity>
    <transaction>
      <new-transaction/>
    </transaction>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <router id="addEditRouter">
      <case id="__1">
        <expression>#{pageFlowScope.selectedTransactionId == ''}</expression>
        <outcome>add</outcome>
      </case>
      <case id="__10">
        <expression>#{pageFlowScope.selectedTransactionId != ''}</expression>
        <outcome>edit</outcome>
      </case>
      <default-outcome>add</default-outcome>
    </router>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <view id="InwardLCTransactionDetail">
      <page>/bfm/InwardLCTransactionDetail.jsff</page>
    </view>
    <view id="SearchInwardLCTransaction">
      <page>/bfm/SearchInwardLCTransaction.jsff</page>
    </view>
    <task-flow-return id="Save">
      <outcome>
        <name>Save</name>
        <commit/>
      </outcome>
    </task-flow-return>
    <task-flow-return id="Cancel">
      <outcome>
        <name>Cancel</name>
        <rollback/>
      </outcome>
    </task-flow-return>
    <method-call id="SetCurrentRowWithKeyValue">
      <method>#{bindings.setCurrentRowWithKeyValue.execute}</method>
      <outcome>
        <fixed-outcome>setCurrentRowWithKeyValue</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__3">
      <from-activity-id>addEditRouter</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>add</from-outcome>
        <to-activity-id>CreateInsert</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__2">
        <from-outcome>edit</from-outcome>
        <to-activity-id>SetCurrentRowWithKeyValue</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__5">
      <from-activity-id>CreateInsert</from-activity-id>
      <control-flow-case id="__6">
        <from-outcome>CreateInsert</from-outcome>
        <to-activity-id>InwardLCTransactionDetail</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__8">
      <from-activity-id>SearchInwardLCTransaction</from-activity-id>
      <control-flow-case id="__9">
        <from-outcome>addEdit</from-outcome>
        <to-activity-id>addEditRouter</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__14">
      <from-activity-id>InwardLCTransactionDetail</from-activity-id>
      <control-flow-case id="__15">
        <from-outcome>submit</from-outcome>
        <to-activity-id>Save</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__16">
        <from-outcome>cancel</from-outcome>
        <to-activity-id>Cancel</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>SetCurrentRowWithKeyValue</from-activity-id>
      <control-flow-case id="__11">
        <from-outcome>setCurrentRowWithKeyValue</from-outcome>
        <to-activity-id>InwardLCTransactionDetail</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
