<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="InwardLCTransactionTaskFlow">
    <default-activity>addEditRouter</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <router id="addEditRouter">
      <case id="__1">
        <expression>#{pageFlowScope.selectedTransactionId == ''}</expression>
        <outcome>add</outcome>
      </case>
      <case id="__2">
        <expression>#{pageFlowScope.selectedTransactionId != ''}</expression>
        <outcome>edit</outcome>
      </case>
      <default-outcome>edit</default-outcome>
    </router>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__3">
      <from-activity-id>addEditRouter</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>add</from-outcome>
        <to-activity-id>CreateInsert</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
